    "import h5py\n",
    "\n",
    "# Abrir un archivo HDF5 existente\n",
    "file = h5py.File('example.h5', 'r')\n",
    "\n",
    "# Leer el dataset\n",
    "dset = file['dataset']\n",
    "\n",
    "# Mostrar datos y atributos\n",
    "print('Datos:', dset[:])\n",
    "print('Descripción:', dset.attrs['description'])\n",
    "\n",
    "# Cerrar el archivo\n",
    "file.close()\n"
   ]
